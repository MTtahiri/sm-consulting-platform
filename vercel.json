{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "saveursmaghrebines.com"
        }
      ],
      "destination": "https://www.saveursmaghrebines.com/$1",
      "permanent": true
    }
  ]
}
